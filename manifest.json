{
    "manifest_version": 2,
    "name": "Ficbook Заявки Сортировка",
    "version": "1.0",
    "description": "Сортирует заявки на Ficbook по количеству лайков",
    "permissions": [
        "https://ficbook.net/*"
    ],
    "content_scripts": [{
        "matches": ["https://ficbook.net/requests*"],
        "js": ["content.js"]
    }],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}